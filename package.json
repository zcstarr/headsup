{
  "name": "headsup",
  "version": "0.0.3",
  "description": "headsup lukso stream",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "restricted"
  },
  "homepage": "https://zcstarr.github.io/nftfloorbot2",
  "repository": {
    "type": "git",
    "url": "https://github.com/zcstarr/nftfloorbot2.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "webpack serve",
    "compile": "hardhat compile",
    "deploy-dev": "hardhat run --network localhost scripts/deploy.ts",
    "refresh": "yarn compile && yarn deploy-dev",
    "setup": "yarn install",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build && yarn lint && yarn test",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' --single-quote --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "build": "webpack --mode production",
    "generate-types": "typechain --target=ethers-v5 'abi/*.json'",
    "postinstall": "yarn generate-types",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^2.2.0",
    "@metamask/eslint-config": "^6.0.0",
    "@metamask/eslint-config-jest": "^6.0.0",
    "@metamask/eslint-config-nodejs": "^6.0.0",
    "@metamask/eslint-config-typescript": "^6.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.2.0",
    "@types/jest": "^26.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.11",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "buffer-browserify": "^0.2.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "hardhat": "^2.10.2",
    "hardhat-gas-reporter": "^1.0.8",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.4.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.2.1",
    "solidity-coverage": "^0.7.21",
    "ts-jest": "^26.3.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4",
    "webpack": "^5.40.0",
    "webpack-cli": "4",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@erc725/smart-contracts": "^3.1.2",
    "@lukso/lsp-smart-contracts": "^0.6.2",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "ethers": "^5.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "typechain": "^8.1.0"
  }
}
